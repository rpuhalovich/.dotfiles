{
  "vim.leader": " ",
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["<c-h>"], "after": ["<c-w>", "h"] },
    { "before": ["<c-j>"], "after": ["<c-w>", "j"] },
    { "before": ["<c-k>"], "after": ["<c-w>", "k"] },
    { "before": ["<c-l>"], "after": ["<c-w>", "l"] },
    { "before": ["<leader>", "p"], "after": ["\"", "+", "p"] },
    { "before": ["<leader>", "a"], "after": [":", "%", "y", "+", "<cr>"] },
    { "before": ["<leader>", "r", "e"], "commands": ["workbench.action.reloadWindow"] },
    { "before": ["<leader>", "s"], "commands": ["workbench.action.splitEditorOrthogonal"] },
    { "before": ["<leader>", "v"], "commands": ["workbench.action.splitEditor"] },
    { "before": ["<c-q>"], "commands": ["workbench.action.closeEditorsInGroup"] },
    { "before": ["K"], "commands": ["editor.action.showHover"] },
    { "before": ["g", "r"], "commands": ["references-view.findReferences"] },
    { "before": ["g", "i"], "commands": ["editor.action.goToImplementation"] },
    { "before": ["s"], "after": [":", "w", "a", "<cr>"] }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": ["<leader>", "y"], "after": ["\"", "+", "y"] }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    { "before": ["{", "<cr>"], "after": ["{", "<cr>", "}", "<esc>", "O"] }
  ],
  "accessibility.verbosity.sourceControl": false,
  "autoHide.autoHideAuxiliaryBar": false,
  "autoHide.autoHidePanel": true,
  "autoHide.autoHideSideBar": true,
  "cSpell.enabled": false,
  "cSpell.mergeCSpellSettings": false,
  "cSpell.useLocallyInstalledCSpellDictionaries": false,
  "chat.commandCenter.enabled": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingComments": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingQuotes": "never",
  "editor.bracketPairColorization.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block",
  "editor.cursorSurroundingLines": 10,
  "editor.folding": false,
  "editor.foldingHighlight": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.guides.indentation": false,
  "editor.inlineSuggest.syntaxHighlightingEnabled": false,
  "editor.lightbulb.enabled": "off",
  "editor.lineNumbers": "off",
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": "off",
  "editor.renderLineHighlight": "none",
  "editor.rulers": [ 160 ],
  "editor.selectionHighlight": false,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.showReferences": false,
  "git.confirmSync": false,
  "git.decorations.enabled": false,
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "scm.diffDecorations": "none",
  "scm.diffDecorationsGutterVisibility": "hover",
  "scm.diffDecorationsGutterWidth": 1,
  "terminal.integrated.stickyScroll.enabled": false,
  "typescript.autoClosingTags": false,
  "update.showReleaseNotes": false,
  "window.commandCenter": false,
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "none",
  "workbench.activityBar.location": "bottom",
  "workbench.editor.restoreViewState": false,
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": null,
  "workbench.navigationControl.enabled": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false
}
