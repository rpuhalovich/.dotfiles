[user]
	email = reanpuhalovich@gmail.com
	name = Ryan Puhalovich
[init]
	defaultBranch = main
[core]
	editor = vim
    pager = less -R
	autocrlf = false
	filemode = false
[push]
	autoSetupRemote = true
[advice]
	detachedHead = false
[diff]
	tool = vimdiff
[difftool]
	prompt = false
	trustExitCode = true
[credential]
	helper = osxkeychain
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
